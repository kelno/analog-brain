{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch debug in Firefox",
        "type": "firefox",
        "request": "launch",
        "reAttach": true,
        "url": "http://localhost:4000",
        "pathMappings": [
          {
            "url": "http://localhost:4000/analog-brain/",
            "path": "${workspaceFolder}/analog-brain-tool/"
          },
        ]
      },
      {
        "name": "Attach debug in Firefox",
        "type": "firefox",
        "request": "attach",
      },
      {
        "name": "Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4000",
      }
    ]
}
